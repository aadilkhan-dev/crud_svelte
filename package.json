		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"prisma": "^4.4.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^4.4.0",
